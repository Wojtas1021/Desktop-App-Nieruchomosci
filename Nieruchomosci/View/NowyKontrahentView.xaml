                                    <CheckBox Grid.Row="1" Grid.Column="2" Content="XML" Style="{StaticResource CheckBoxStandardowy}"/>
                                    <Label Grid.Row="2" Grid.Column="0" Content="ID Partnera:" Style="{StaticResource LabelLewy}"/>
                                    <Label Grid.Row="3" Grid.Column="0" Content="Odział Partnera:" Style="{StaticResource LabelLewy}"/>
                                </Grid>
                            </Expander>
                        </Grid>
                    </Grid>
                    <Expander Grid.Row="2" Grid.Column="0" Header="Opis" Grid.ColumnSpan="3" Style="{StaticResource ExpanderRozwiniety}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
