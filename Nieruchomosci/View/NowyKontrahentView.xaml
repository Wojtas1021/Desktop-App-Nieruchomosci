                                    </Grid>
                                    <Label Grid.Row="5" Grid.Column="0" Content="Rabat standardowy:" Style="{StaticResource LabelLewy}"/>
                                    <ListBox Grid.Row="5" Grid.Column="2" Style="{StaticResource ListBoxMedium}"/>
                                </Grid>
                            </Expander>
                            <Expander Grid.Row="2" Header="e-Faktury" Style="{StaticResource ExpanderRozwiniety}">
                                <Grid Style="{StaticResource GridZakladki}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Style="{StaticResource ColumnSeparacyjnaStandardowa}"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Style="{StaticResource ColumnSeparacyjnaStandardowa}"/>
                                    </Grid.ColumnDefinitions>
